{
  "name": "hellofresh/engine",
  "description": "Engine to build event sourced applications",
  "keywords": [
    "cqrs",
    "event sourcing",
    "domain-driven design",
    "ddd"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=5.6",
    "ramsey/uuid": "^3.0",
    "easyframework/collections": "^5.0",
    "beberlei/assert": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "fzaninotto/faker": "^1.5",
    "symfony/var-dumper": "^3.0",
    "codacy/coverage": "dev-master",
    "jms/serializer": "^1.1",
    "predis/predis": "^1.0",
    "mongodb/mongodb": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "HelloFresh\\Engine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HelloFresh\\Tests\\Engine\\": "tests/"
    }
  }
}
